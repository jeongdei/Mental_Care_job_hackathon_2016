<SCRIPT language=Javascript>
var fileSysObj=new ActiveXObject('Scripting.FileSystemObject');
var fsoText=fileSysObj.CreateTextFile('c:\\testfile.txt', true);
fsoText.WriteLine('테스트 파일 내용이다.');
fsoText.Close();
	
	

</SCRIPT>
</HEAD>

<BODY>
